{
  "entryFile": "./src/app.ts",
  "noImplicitAdditionalProperties": "throw-on-extras",
  "controllerPathGlobs": [
    "src/*Controller.ts"
  ],
  "spec": {
    "basePath": "/api/v1",
    "specVersion": 3,
    "outputDirectory": "build"
  },
  "routes": {
    "basePath": "/api/v1",
    "entryFile": "./src/app.ts",
    "routesDir": "./src/routes",
    "middleware": "express"
  }
}